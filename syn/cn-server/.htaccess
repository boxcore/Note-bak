<IfModule mod_deflate.c>
	SetOutputFilter DEFLATE
	AddOutputFilterByType DEFLATE text/html text/css application/x-javascript
</IfModule>
<FilesMatch "\.(bak|inc|lib|sh|tpl|lbi|dwt)$">
    order deny,allow
    deny from all
</FilesMatch>

RewriteEngine On
#RewriteBase /

RewriteEngine On
RewriteCond %{HTTP_HOST} ^lvbaopifa.com [NC,OR]
RewriteCond %{HTTP_HOST} ^wmdpifa.gotoip55.com [NC]
RewriteRule ^(.*)$ http://mall.dswf.cn/$1 [L,R=301]
RewriteRule ^http://mall.dswf.cn/index.php/$ http://www.lvbaopifa.com/$1 [L,R=301]
